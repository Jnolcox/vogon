      <columns>
        <TableColumn fx:id="columnSourceCurrency" editable="false" prefWidth="75.0" text="%CURRENCY_SOURCE">
          <cellValueFactory>
            <PropertyValueFactory property="sourceCurrency" />
          </cellValueFactory>
        </TableColumn>
        <TableColumn fx:id="columnDestinationCurrency" editable="false" prefWidth="75.0" text="%CURRENCY_DESTINATION">
          <cellValueFactory>
            <PropertyValueFactory property="destinationCurrency" />
          </cellValueFactory>
        </TableColumn>
        <TableColumn fx:id="columnExchangeRate" prefWidth="75.0" text="%CURRENCY_EXCHANGE_RATE">
          <cellValueFactory>
            <PropertyValueFactory property="exchangeRate" />
          </cellValueFactory>
        </TableColumn>
      </columns>
    </TableView>
  </children>
